{
    "arch": "arm",
    "cpu": "cortex-a8",
    "data-layout": "e-m:e-p:32:32-i64:64-v128:64:128-a:0:32-n32-S64",
    "disable-redzone": true,
    "executables": true,
    "llvm-target": "thumbv7a-none-eabi",
    "morestack": false,
    "os": "none",
    "relocation-model": "static",
    "target-endian": "little",
    "target-pointer-width": "32",
    "no-compiler-rt": true,
    "features": "+v7,+vfp3,+d16,+neon",
    "pre-link-args": [
        "-mcpu=cortex-a8", "-mthumb", "-mfloat-abi=hardfp"
    ],
    "post-link-args": [
        "-lm", "-lgcc", "-lnosys"
    ]
}
